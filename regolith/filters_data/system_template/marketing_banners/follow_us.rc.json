{
	"format_version": "1.10.0",
	"render_controllers": {
		"controller.render.shapescape.follow_us": {
			"geometry": "Geometry.default",
			"filter_lighting": true,
			"materials": [{ "*": "Material.triple" }],
			"textures": ["Texture.background", "Texture.background", "Texture.frame"]
		},
		"controller.render.shapescape.follow_us_text": {
			"geometry": "Geometry.default",
			"filter_lighting": true,
			"materials": [{ "*": "Material.single" }],
			"textures": ["Array.skins[query.life_time * 20]"],
			"arrays": {
				"textures": {
					"Array.skins": "`[f'texture.f{i}' for i in range(follow_us_frame_count)]`"
				}
			}
		}
	}
}
